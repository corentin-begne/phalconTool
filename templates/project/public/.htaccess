AddDefaultCharset UTF-8

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_URI} \..+$
    RewriteCond %{REQUEST_URI} !\.html$
    RewriteRule .* - [L]
    RewriteRule ^(.*)$ /[app]/index.php?_url=/$1 [QSA,L]
</IfModule>

<IfModule mod_mime.c>
  AddType text/javascript js mjs
</IfModule>